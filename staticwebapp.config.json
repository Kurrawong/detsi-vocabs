{
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": [
      "*.{css,scss,sass,js,ts,tsx,jsx,json,png,jpg,jpeg,gif,bmp,svg,ico,woff,woff2,ttf,eot,otf,webp,avif}",
      "*.{html,htm,xml,txt,md,pdf,doc,docx,ppt,pptx,xls,xlsx,zip,tar,gz,rar,7z}"
    ]
  },
  "responseOverrides": {
    "404": {
      "rewrite": "/index.html"
    }
  }
}
